// Define your emergency() function. You should replace this with your actual implementation.
int emergency() {
    return 1; // Change this to return the appropriate value for your test.
}

// Define a test case function
TEST(EmergencyTest, BasicAssertions) {
    EXPECT_EQ(emergency(), 1); // Change 1 to the expected result (e.g., Stop) in your test.
}

int main(int argc, char **argv) {
